class MovieRentingSystem {
public:

    unordered_map<int,set<pair<int,int>>> ms; //movie->shop,price
    unordered_map<int,set<pair<int,int>>> available; //movie->price,shop
    set<tuple<int,int,int>> rented; //price,shop,movie
    MovieRentingSystem(int n, vector<vector<int>>& entries) {
        for(auto &i: entries){
            int shop = i[0];
            int movie = i[1];
            int price = i[2];

            ms[movie].insert({shop,price});
            available[movie].insert({price,shop});
        }
    }
    
    vector<int> search(int movie) {
       int count = 0;
       vector<int> result;

       if(available.count(movie)){
        for(auto &[price,shop]: available[movie]){
            result.push_back(shop);
            count++;
            if(count==5){
                break;
            }
        }
       } 

       return result;
    }
    
    void rent(int shop, int movie) {
        auto it = ms[movie].lower_bound({shop,-1});
        auto price = it->second;
        available[movie].erase({price,shop});
        rented.insert({price,shop,movie});
    }
    
    void drop(int shop, int movie) {
        auto it = ms[movie].lower_bound({shop,-1});
        auto price = it->second;
        available[movie].insert({price,shop});
        rented.erase({price,shop,movie});
        
    }
    
    vector<vector<int>> report() {
        int count = 0;
        vector<vector<int>> result;
        for(auto &[price,shop,movie]: rented){
            result.push_back({shop,movie});
            count++;

            if(count==5){
                break;
            }
        }

        return result;
    }
};

